  <style>
    body {
      display: flex;
      justify-content: center;
      align-items: center;
      min-height: 100vh;
      margin: 0;
      background-color: #f0f0f0;
      font-family: sans-serif;
    }

    .container {
      display: grid;
      grid-template-columns: repeat(4, 1fr);
      /* 4 columns */
      gap: 20px;
      padding: 20px;
      background-color: #fff;
      border-radius: 10px;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    }

    .icon {
      width: 100px;
      /* Adjust as needed */
      height: 100px;
      /* Adjust as needed */
      border-radius: 10px;
      background-color: #eee;
      display: flex;
      justify-content: center;
      align-items: center;
      cursor: pointer;
      transition: background-color 0.3s ease;
      overflow: hidden;
      /* Hide image overflow */
    }

    .icon:hover {
      background-color: #ddd;
    }

    .icon img {
      width: 100%;
      /* Image fills the container */
      height: 100%;
      /* Image fills the container */
      object-fit: cover;
      /* Maintain aspect ratio and cover the area */
    }

    .icon.active {
      background-color: #007bff;
    }

    .icon.active img {
      opacity: 0.5;
      /* Make the image slightly transparent when active */
    }
  </style>
</head>

<body>
  <div class="container">
    <div class="icon" onclick="toggleIcon(this); playSound('sound1')">
      <img src="https://www.jocooks.com/wp-content/uploads/2020/03/white-bread-1-500x375.jpg" alt="Icon 1">
      <audio id="sound1" src="r7slpCHAUz3S5RGQ4eBThYRqkp4iHN4BUL3P (1).mp3"></audio>
      <script>
        function toggleIcon(this) {
          icon.this.toggle('active');
        }
        function playSound("sound1") {
          const audio = document.getElementById("sound1");
          audio.play("sound1");
        }
      </script>

